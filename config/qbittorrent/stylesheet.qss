  padding: 0px;
  border: 2px solid #39BAE6;
}

QToolBox::tab {
  background-color: #2A2F3A;
  border: 1px solid #273747;
  color: #B3B1AD;
  border-top-left-radius: 4px;
  border-top-right-radius: 4px;
}

QToolBox::tab:disabled {
  color: #626A73;
}

QToolBox::tab:selected {
  background-color: #232933;
  border-bottom: 2px solid #39BAE6;
}

QToolBox::tab:selected:disabled {
  background-color: #273747;
  border-bottom: 2px solid #59C2FF;
}

QToolBox::tab:!selected {
  background-color: #273747;
  border-bottom: 2px solid #273747;
}

QToolBox::tab:!selected:disabled {
  background-color: #2A2F3A;
}

QToolBox::tab:hover {
  border-color: #F07178;
  border-bottom: 2px solid #F07178;
}

QToolBox QScrollArea QWidget QWidget {
  padding: 0px;
  border: 0px;
  background-color: #2A2F3A;
}

/* QFrame -----------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qframe

--------------------------------------------------------------------------- */
.QFrame { /* Not Main window */
  border-radius: 4px;
  border: 1px solid #273747;
}

.QFrame[frameShape="0"] {
  border-radius: 4px;
  border: 1px transparent #273747;
}

.QFrame[height="3"], .QFrame[width="3"] {
  background-color: #2A2F3A; /* Right Bottom pain separator, Statusbar separator*/
  border: 1px solid #2A2F3A; /* Add */
  width: 0px;  height: 0px;  margin: 0px;  padding: 0px; /* Add */
}

