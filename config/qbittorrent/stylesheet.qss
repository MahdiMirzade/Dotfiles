/* QMenu ------------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qmenu

--------------------------------------------------------------------------- */
QMenu {
  color: #B3B1AD;
  margin: 0px;
  padding: 0px;
  border: 1px solid #273747; /* Add */
}

QMenu::separator {
  height: 1px;
  background-color: #273747;
  color: #B3B1AD;
}

QMenu::item {
  padding: 4px; /* change */
  padding-left: 4px; /* Add */
  padding-right: 20px; /* Add */
  /* Reserve space for selection border */ /* delete
  border: 1px solid transparent; */
}

QMenuBar QMenu::item {
  padding-left: 3px; /* Add */
}

QMenuBar QMenu::item QMenu::item { /* sub menu */
  padding-left: 4px; /* Add */
}

QMenu::item:selected,
QMenu::item:hover { /* Add */
  color: #39BAE6; /* change */
  background-color: #232933;
}

QMenuBar QMenu::item:selected,
QMenuBar QMenu::item:hover { /* Add elm */
  color: #C2D94C;
}

QMenu::icon {
  margin: 0px;
  padding-left: 3px; /* change */
}

QMenu::indicator {
  width: 12px;
  height: 12px;
  padding-top: 2px; /* Add*/
  padding-left: 4px; /* change */
  /* non-exclusive indicator = check box style indicator (see QActionGroup::setExclusive) */
  /* exclusive indicator = radio button style indicator (see QActionGroup::setExclusive) */
}

/* reverse : correct order = checked, unchecked, otherwise always checked*/
QMenu::indicator:non-exclusive:checked { /* Menu checkbox: checked */
  image: url(":/icons/qss/checkbox_checked.png");
}
QMenu::indicator:non-exclusive:checked:selected { /* Menu checkbox: check focus */
  image: url(":/icons/qss/checkbox_checked_focus.png");/* change */
}
QMenu::indicator:non-exclusive:unchecked { /* Menu checkbox: uncheck */
  image: url(":/icons/qss/checkbox_unchecked.png");
}
QMenu::indicator:non-exclusive:unchecked:selected { /* Menu checkbox: uncheck focus */
  image: url(":/icons/qss/checkbox_unchecked_focus.png"); /* change */
}

QMenu::indicator:exclusive:checked {
  image: url(":/icons/qss/radio_checked.png");
}
QMenu::indicator:exclusive:checked:selected {
  image: url(":/icons/qss/radio_checked_focus.png");
}
QMenu::indicator:exclusive:unchecked {
  image: url(":/icons/qss/radio_unchecked.png");
}
QMenu::indicator:exclusive:unchecked:selected {
  image: url(":/icons/qss/radio_unchecked_focus.png");
}

QMenu::right-arrow {
  margin: 5px;
  image: url(":/icons/qss/arrow_right.png");
  height: 12px;
  width: 12px;
}


