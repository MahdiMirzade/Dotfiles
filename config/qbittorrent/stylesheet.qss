/* QHeaderView ------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qheaderview

--------------------------------------------------------------------------- */
QHeaderView {
  background-color: #273747;
  border: 0px transparent #273747;
  border-bottom: 1px solid #626A73;
  padding: 0px;
  margin: 0px;
  border-radius: 0px;
}

QHeaderView:disabled {
  background-color: #273747;
  border: 1px transparent #273747;
  padding: 0px; /* change */
}

QHeaderView::section { /* Memo : transfer list table heading */
  background-color: #273747;
  color: #B3B1AD;
  padding: 2px;
  border-radius: 0px;
  text-align: left;
}

QHeaderView::section:checked {
  color: #B3B1AD;
  background-color: #59C2FF;
}

QHeaderView::section:checked:disabled {
  color: #626A73;
  background-color: #59C2FF;
}

QHeaderView::section::horizontal {
  padding: 0px 4px; /* Add */
  border-left: 1px solid #626A73;
}

QHeaderView::section::horizontal::first, QHeaderView::section::horizontal::only-one {
  border-left: 0px solid #626A73; /* change */
}

QHeaderView::section::horizontal:disabled {
  color: #626A73;
}

QHeaderView::section::vertical {
  border-top: 1px solid #626A73; /* change */
}

QHeaderView::section::vertical::first, QHeaderView::section::vertical::only-one {
  border-top: 1px solid #273747;
}

QHeaderView::section::vertical:disabled {
  color: #626A73;
}

QHeaderView::down-arrow {
  /* Those settings (border/width/height/background-color) solve bug */
  /* transparent arrow background and size */
  background-color: #273747;
  color: #626A73;
  height: 12px;
  width: 12px;
  image: url(":/icons/qss/arrow_down.png"); /* used */
}

QHeaderView::up-arrow {
  background-color: #273747;
  height: 12px;
  width: 12px;
  image: url(":/icons/qss/arrow_up.png"); /* used */
}


